			_this.parent('.StaticPolicySelect-item').addClass('is-Current').siblings('.StaticPolicySelect-item').removeClass('is-Current'); 
			_this.closest('.StaticPolicySelect-list').stop().slideUp(100).prev('.StaticPolicySelect-text').text(this_text);
