// IFrame Auto Height
//==================================================*
if(jQuery('#autoIframe').length > 0){
	function iframeHeight() { 
		var the_height = document.getElementById('autoIframe').contentWindow. document.body.scrollHeight; 
		document.getElementById('autoIframe').height = the_height; 
		document.getElementById('autoIframe').style.overflow = "hidden"; 
	}
}

(function($){
	var agent = navigator.userAgent.toLowerCase(); 

	if ( (navigator.appName == 'Netscape' && navigator.userAgent.search('Trident') != -1) || (agent.indexOf("msie") != -1) ) {
		if ($('.single-hws_video source').length > 0) {
			$('.single-hws_video video')[0].pause();
			var url = $('.single-hws_video source').attr('src');
			var url  = url.replace('http://','https://');
			$('.single-hws_video source').attr('src', url)
			$('.single-hws_video video')[0].load();
			$('.single-hws_video video')[0].play();
		}
	}
})(jQuery);
