		 * cybersecurityBtnView
		 */
		if($('.CyberSecurityPs').length > 0){
			function cybersecurityBtnView() {
				$( '.FilePostList-list' ).each( function() {
					if($(window).width() > 1024){
						if ( $( '.FilePostList-item', this ).length > 3 ) {
							$(this).closest('.CyberSecurityPs').addClass('View-More');
						}else{
							$(this).closest('.CyberSecurityPs').removeClass('View-More');
						}
					}else{
						if ( $( '.FilePostList-item', this ).length > 2 ) {
							$(this).closest('.CyberSecurityPs').addClass('View-More');
						}else{
							$(this).closest('.CyberSecurityPs').removeClass('View-More');
						}
					}
				});
			}

			$(window).bind('resize', cybersecurityBtnView);
			cybersecurityBtnView();
		}
		
		
