			  $(this).parent('li').addClass('is-Current').siblings('li').removeClass('is-Current');
			  $('.StaticPolicyContItem').eq(idx).addClass('is-Current').siblings('.StaticPolicyContItem').removeClass('is-Current');
			return false;
