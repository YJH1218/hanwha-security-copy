		// PopLanguageOn
		//==================================================*
		function PopLanguageOn() {
			if($('.PopLanguageSection').hasClass('is-View')){
				$('.PopLanguageSection').removeClass('is-View');
				$('html').removeClass('is-Hidden');
			}else{
				$('.PopLanguageSection').addClass('is-View');
				$('html').addClass('is-Hidden');
				$('.SiteMap').removeClass('is-View');
				$('.HeaderSearch').removeClass('is-Open');
				$('.MobileHeaderSearch').removeClass('is-Open');
				$('.QuickContact').removeClass('is-View');
				$('.QuickDownload').removeClass('is-View');
			}
			return false;
		}
		
		$(document).on('click', '.HeaderLang-text, .MobileHeader-link.lang', PopLanguageOn);
		
		// PopLanguageOff
		//==================================================*
		function PopLanguageOff() {
			  $('.PopLanguageSection').removeClass('is-View');
			  $('html').removeClass('is-Hidden');
			return false;
		}
		
		$(document).on('click', '.PopLanguageHeader-close, .PopLanguageSectionBg', PopLanguageOff);
		
		// Checkbox Check(All)
		//==================================================*
		/**
		 * Checkbox Check(All): All Check
		 */
		function allCheck() {
			var label = $(this).prev('input[type="checkbox"]');
			$(label).change(function(){
				if($(label).is( ":checked") == true) {
					$('[name="chkbox"]:not(:disabled)').prop("checked",true);
				} else {
					$('[name="chkbox"]:not(:disabled)').prop("checked",false);
				}
			});
		};
		$(document).on('click', '.Check-all', allCheck);
		
		/**
		 * Checkbox Check(All): Item Check
		 */
		function allCheckItem() {
			var label = $(this).prev(".DataCheckBox-input");
			var subInput = $('.DataCenterTbody [name="chkbox"]:not(:disabled)');
			
			$(label).change(function(){
				if (subInput.length > subInput.filter(":checked").length) {
					$('[name="chkall"]').prop("checked",false);
				} else {
					$('[name="chkall"]').prop("checked",true);
				}
			});
		};
		$(document).on('click', '.DataCenterTbody .DataCheckBox-label', allCheckItem);
		
		// DataCenter Drop
		//==================================================*
		function DataCenterDrop() {
			 var idx = $(this).parent('li').index();
			 var selectName = $(this).closest('.DataCenterItem').find('.DataCenterSelect option').eq(idx).val();
			$(this).parent('.DataCenterItemProductContMeta-item').addClass('is-Current').siblings('.DataCenterItemProductContMeta-item').removeClass('is-Current');
			$(this).closest('.DataCenterItemProductContMeta').find('.DataCenterItemProductContMeta-arrow').addClass('is-Open');
			$(this).closest('.DataCenterItem').find('.DataCenterSelect option').eq(idx).prop('selected', true);
			$(this).closest('.DataCenterItem').find('.DataCenterSelect').val(selectName);
			$(this).closest('.DataCenterItem').find('.DataCenterItemInfo').slideDown(120);
		};
		$(document).on('click', '.DataCenterItemProductContMeta-link', DataCenterDrop);
		
		function DataCenterArrowDrop() {
			if($(this).hasClass('is-Open')){
				$(this).removeClass('is-Open');
				$(this).closest('.DataCenterItemProductContMeta').find('.DataCenterItemProductContMeta-item').removeClass('is-Current');
				$(this).closest('.DataCenterItem').find('.DataCenterItemInfo').slideUp(120);
			}else{
				$(this).addClass('is-Open');
				$(this).closest('.DataCenterItemProductContMeta').find('.DataCenterItemProductContMeta-item:first-child').addClass('is-Current');
				$(this).closest('.DataCenterItem').find('.DataCenterItemInfo').slideDown(120);
			}
		};
		$(document).on('click', '.DataCenterItemProductContMeta-arrow', DataCenterArrowDrop);
		
		function DataOptionChange() {
			var selectOption = $(this).find('option:selected');
			 var idx = $(this).find('option').index(selectOption);
			 $(this).closest('.DataCenterItem').find('.DataCenterItemProductContMeta-item').eq(idx).addClass('is-Current').siblings('.DataCenterItemProductContMeta-item').removeClass('is-Current');
		};
		$(document).on('change', '.DataCenterSelect', DataOptionChange);
	});
	
	 if($('.single-hws_case_study .BasicSingleCont').length > 0){
		 $('.single-hws_case_study .BasicSingleCont p:contains("♦")').addClass('k_para_title').parent().removeClass('k_para_title');
		  $('.single-hws_case_study .BasicSingleCont div:contains("♦")').addClass('k_para_title').parent().removeClass('k_para_title');
		  $('.single-hws_case_study .BasicSingleCont span:contains("♦")').addClass('k_para_title').parent().removeClass('k_para_title');
		  $('.single-hws_case_study .BasicSingleCont .k_para_desc_mb:contains("♦")').addClass('k_para_title_v2').parent().removeClass('k_para_title');
	 }
	
