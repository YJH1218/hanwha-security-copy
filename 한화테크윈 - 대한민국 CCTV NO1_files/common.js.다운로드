(function($){
	//==================================================*
	//	Common
	//==================================================*
	/**
	 * Common:ScrollMagic Controller
	 */
	var controller = new ScrollMagic.Controller();

	//==================================================*
	//	Document:Ready
	//==================================================*
	$(document).ready(function(){
		
				
		//TopBtn 
		//==================================================*
		$(window).scroll(function () {
			if ($(this).scrollTop() > 200) {
				$('.TopButton').fadeIn(200);
			} else {
				$('.TopButton').fadeOut(200);
			}
		});
		$('.TopButton').click(function () {
			$('html, body').animate({scrollTop: 0}, 400);
			return false;
		});
		

		
		
		
		// LinkDefault
		//==================================================*
		function linkDefault() {
			return false;
		}
		
		$(document).on('click', '[class*="js-"]', linkDefault);

		/**
		 * ScrollMagic: Opacity
		 */
		if($('.a-Opacity').length > 0){
			$('.a-Opacity').each(function(){
				var Opacity = new ScrollMagic.Scene({
					triggerElement: this.children[0],
					triggerHook:0.9
				})
				.reverse(false)
				.setClassToggle(this, 'is-Opacity')
				.addTo(controller);
			});
		}
		
		/**
		 * ScrollMagic: OpacityTop
		 */
		if($('.a-OpacityTop').length > 0){
			$('.a-OpacityTop').each(function(){
				var OpacityTop = new ScrollMagic.Scene({
					triggerElement: this.children[0],
					triggerHook:0.9
				})
				.reverse(false)
				.setClassToggle(this, 'is-OpacityTop')
				.addTo(controller);
			});
		}
		
		// Family Site
		//==================================================*
		function FamilyCustomSelect() {
			if($('.FooterFamilyMenu').hasClass('is-VIew')){
				$('.FooterFamilyMenu').removeClass('is-VIew').slideUp(120);
			}else{
				$('.FooterFamilyMenu').addClass('is-VIew').slideDown(120);
				$("html, body").animate({ scrollTop: $(document).height() }, 120);
			}
		}
		
		$(document).on('click', '.js-FamilySite', FamilyCustomSelect);
		
		function FamilyTab() {
		  var idx = $(this).parent('li').index();
		  $(this).parent('li').addClass('is-Current').siblings('li').removeClass('is-Current');
		  $('.FooterFamilyMenuTabCont-item').eq(idx).addClass('is-Current').siblings('.FooterFamilyMenuTabCont-item').removeClass('is-Current');
		}
		$(document).on('click', '.FooterFamilyMenuTab-link', FamilyTab);
		
		function FamilyTabClose() {
		  $('.FooterFamilyMenu').removeClass('is-VIew').slideUp(120);
		}
		$(document).on('click', '.FooterFamilyMenu-close', FamilyTabClose);
		
		//Header Fixed
		//==================================================*
		function headerFixed(){
			if ($(this).scrollTop() > 0) {
				$('body').addClass('is-HeaderFixed');
			} else {
				$('body').removeClass('is-HeaderFixed');
				$('.HeaderSearchButton').removeClass('is-View');
				$('.HeaderSearch').removeClass('is-View');
			}
		};
		
		$(document).ready(function() {
			headerFixed();
			$(window).scroll(function(){
				headerFixed();
			});
		});
		
		var timer;
		$(window).bind('resize', headerFixed);
		
		//Header Search On
		//==================================================*
		function HeaderSearchBtn() {
			var Select = $(this);
			if(Select.hasClass('is-View')){
				Select.removeClass('is-View').closest('.site-header').find('.HeaderSearch').removeClass('is-View');
			}else{
				Select.addClass('is-View').closest('.site-header').find('.HeaderSearch').addClass('is-View');
			}
		}
		
		$(document).on('click', '.HeaderSearchButton', HeaderSearchBtn);
		
		$(document).mouseup(function (e){
				if(!$('.site-header, .HeaderSearch').is(e.target) && $('.site-header, .HeaderSearch').has(e.target).length === 0){
						$('.HeaderSearch').removeClass('is-View').closest('.site-header').find('.HeaderSearchButton').removeClass('is-View')
				}
		});
		
		//Mobile Search On
		//==================================================*
		function MobileSearchBtn() {
			var Select = $(this);
			if(Select.hasClass('is-Open')){
				Select.removeClass('is-Open').closest('.site').find('.HeaderSearch').removeClass('is-Open');
			}else{
				Select.addClass('is-Open').closest('.site').find('.HeaderSearch').addClass('is-Open');
				$('.SiteMap').removeClass('is-View');
				$('html').removeClass('is-Hidden');
				$('.QuickContact').removeClass('is-View');
				$('.QuickDownload').removeClass('is-View');
				$('.PopLanguageSection').removeClass('is-View');
			}
		}
		
		$(document).on('click', '.MobileHeaderSearch', MobileSearchBtn);
		
		$(document).mouseup(function (e){
				if(!$('.site, .MobileHeader').is(e.target) && $('.site, .MobileHeader').has(e.target).length === 0){
						$('.HeaderSearch').removeClass('is-Open').closest('.site').find('.MobileHeaderSearch').removeClass('is-Open')
				}
		});
		
		//TopBtn
		//==================================================*
		//function TopBtn() {
		//	$('html,body').animate({
		//		scrollTop:0
		//	}, 500);
		//}
		
		//$(document).on('click', '.TopButton', TopBtn);
		
		//SiteChildren
		//==================================================*
		$(function($){
			var clickAllowed = true;

			$('.SiteMapMenu .menu-item-has-children a').click(function(){
				if (clickAllowed) {
					if($(this).parent('.menu-item-has-children').hasClass('is-Open')){
						$(this).parent('.menu-item-has-children').removeClass('is-Open');
						$(this).next('.sub-menu').slideUp(120);
					}else{
						$(this).parent('.menu-item-has-children').addClass('is-Open');
						$(this).next('.sub-menu').slideDown(120);
					}
					
					if($(this).parent().has('ul').length == 0){
							return true;
					}
					
					return false;
				}
			});
			function onResize() {
				if($(window).width() >= 1025){
					clickAllowed = true;
				}
				else{
					clickAllowed = false;
				}
			}
			onResize();
			var timer;
			$(window).bind('resize', onResize);
		});
		
		//SiteClose
		//==================================================*
		function SiteClose() {
			$('.SiteMap').removeClass('is-View');
			$('html').removeClass('is-Hidden');
		}
		
		$(document).on('click', '.SiteMapHeader-close', SiteClose);
		
		//SiteOpen
		//==================================================*
		function SiteOpen() {
			$('.SiteMap').addClass('is-View');
			$('html').addClass('is-Hidden');
		}
		
		$(document).on('click', '.HeaderMegaMenu', SiteOpen);
		
		//MobileSiteOpen
		//==================================================*
		function MobileSiteOpen() {
			if($('.SiteMap').hasClass('is-View')){
				$('.SiteMap').removeClass('is-View');
				$('html').removeClass('is-Hidden');
			}else{
				$('.SiteMap').addClass('is-View');
				$('html').addClass('is-Hidden');
				$('.HeaderSearch').removeClass('is-Open');
				$('.MobileHeaderSearch').removeClass('is-Open');
				$('.QuickContact').removeClass('is-View');
				$('.QuickDownload').removeClass('is-View');
				$('.PopLanguageSection').removeClass('is-View');
			}
		}
		
		$(document).on('click', '.MobileHeader .MobileHeader-link.menu', MobileSiteOpen);
		
		/**
		 * Mobile Nav Toggle
		 */
		 $(function($){
			var clickAllowed = true;
			var this_index = $(this).parent('li').index();

			$('.SiteMapMenu .main-menu > .menu-item  a').click(function(){
				if (clickAllowed) {
					$(this).parent('li').toggleClass('is-View').siblings('li').removeClass('is-View');

					if($(this).parent().has('ul').length == 0){
							return true;
					}
					$(this).siblings('ul').stop().slideToggle(160).parent('li').siblings('li').find('ul').stop().slideUp(160).parent('li').removeClass('is-View');

					return false;
				}
			});
			function onResize() {
				if($(window).width() <= 1024){
					clickAllowed = true;
				}
				else{
					clickAllowed = false;
				}
			}
			onResize();
			var timer;
			$(window).bind('resize', onResize);
		});
		
		// CustomSelect
		//==================================================*
		function CustomSelect() {
			var Select = $(this).parent('.BasicSelect');
			if(Select.hasClass('is-Open')){
				Select.removeClass('is-Open').children('.BasicSelect-list').stop().slideUp(140);
			}else{
				$('.BasicSelect').removeClass('is-Open').children('.BasicSelect-list').stop().slideUp(140);
				Select.addClass('is-Open').children('.BasicSelect-list').stop().slideDown(140);
			}
		}
		
		$(document).on('click', '.BasicSelect-text', CustomSelect);
		
		
		$(document).mouseup(function (e){
				if(!$('.BasicSelect').is(e.target) && $('.BasicSelect').has(e.target).length === 0){
					$('.BasicSelect').removeClass('is-Open').children('.BasicSelect-list').stop().slideUp(140);
				}
		});
		
		// PostSelect
		//==================================================*
		function PostSelect() {
			var Select = $(this).parent('.PostSelect');
			if(Select.hasClass('is-Open')){
				Select.removeClass('is-Open').children('.PostSelect-list').stop().slideUp(140);
			}else{
				$('.PostSelect').removeClass('is-Open').children('.PostSelect-list').stop().slideUp(140);
				Select.addClass('is-Open').children('.PostSelect-list').stop().slideDown(140);
			}
		}
		
		$(document).on('click', '.PostSelect-text', PostSelect);
		
		function PostSelectOption() {
			var _this = $(this);
			var this_text = _this.text();
			_this.closest('.PostSelect').removeClass('is-Open');
			_this.parent('.PostSelect-item').addClass('is-Current').siblings('.PostSelect-item').removeClass('is-Current'); 
			_this.closest('.PostSelect-list').stop().slideUp(100).prev('.PostSelect-text').text(this_text);
			_this.closest('.PostSelect-list').stop().slideUp(100);
			return false;
		}
		
		$(document).on('click', '.PostSelect-link', PostSelectOption);
		
		$(document).mouseup(function (e){
				if(!$('.PostSelect').is(e.target) && $('.PostSelect').has(e.target).length === 0){
					$('.PostSelect').removeClass('is-Open').children('.PostSelect-list').stop().slideUp(140);
				}
		});
		
		// FormSelect
		//==================================================*
		function FormSelect() {
			var Select = $(this).parent('.FormSelect');
			if(Select.hasClass('is-Open')){
				Select.removeClass('is-Open').children('.FormSelect-list').stop().slideUp(140);
			}else{
				$('.FormSelect').removeClass('is-Open').children('.FormSelect-list').stop().slideUp(140);
				Select.addClass('is-Open').children('.FormSelect-list').stop().slideDown(140);
			}
		}
		
		$(document).on('click', '.FormSelect-text', FormSelect);
		
		function FormSelectOption() {
			var _this = $(this);
			var this_text = _this.text();
			_this.closest('.FormSelect').removeClass('is-Open');
			_this.parent('.FormSelect-item').addClass('is-Current').siblings('.FormSelect-item').removeClass('is-Current'); 
			_this.closest('.FormSelect-list').stop().slideUp(100).prev('.FormSelect-text').text(this_text);
			_this.closest('.FormSelect-list').stop().slideUp(100);
			return false;
		}
		
		$(document).on('click', '.FormSelect-link', FormSelectOption);
		
		$(document).mouseup(function (e){
				if(!$('.FormSelect').is(e.target) && $('.FormSelect').has(e.target).length === 0){
					$('.FormSelect').removeClass('is-Open').children('.FormSelect-list').stop().slideUp(140);
				}
		});
		
		 // Sub Menu Slide
		//==================================================*
		 if($('.MenuCaseStudySlide').length > 0){
			var MenuCaseStudySlideSwiper = new Swiper('.MenuCaseStudySlide-container', {
				slidesPerView: 1,
				observer: true,
				observeParents: true,
				watchOverflow: true,
				watchSlidesProgress: true,
				navigation: {
					nextEl: '.MenuCaseStudySlide-next',
					prevEl: '.MenuCaseStudySlide-prev'
				},
			});
		}
		
		// Menu Open
		//==================================================*
		/**
		 * Menu Open: mouseover
		 */
		function NaviBarOver() {
		  $('.site-header').addClass('is-Menu');
		}
		$(document).on('mouseover', '.site-header .main-navigation, .DepthMenu', NaviBarOver);
		
		/**
		 * Menu Open: mouseleave
		 */
		function NaviBarLeave() {
		  $('.site-header').removeClass('is-Menu');
		   $('.HeaderNav .main-menu > .menu-item').removeClass('is-Hover');
		}
		$(document).on('mouseleave', '.main-navigation, .DepthMenu', NaviBarLeave);
		
		// Quick Contact
		//==================================================*
		function QuickContact() {
			if($('.QuickContact').hasClass('is-View')){
				$('.QuickContact').removeClass('is-View');
			}else{
				$('.QuickContact').addClass('is-View');
				$('.QuickDownload').removeClass('is-View');
				$('.SiteMap').removeClass('is-View');
				$('html').removeClass('is-Hidden');
				$('.HeaderSearch').removeClass('is-Open');
				$('.MobileHeaderSearch').removeClass('is-Open');
				$('.PopLanguageSection').removeClass('is-View');
			}
		}
		
		$(document).on('click', '.QuickBarItem-contact .QuickBarItem-link, .MobileHeader-link.contact', QuickContact);
		
		function QuickContactClose() {
			$('.QuickContact').removeClass('is-View');
		}
		
		$(document).on('click', '.QuickContact-close', QuickContactClose);
		
		// Quick Download
		//==================================================*
		function QuickDownload() {
			if($('.QuickDownload').hasClass('is-View')){
				$('.QuickDownload').removeClass('is-View');
			}else{
				$('.QuickDownload').addClass('is-View');
				$('.QuickContact').removeClass('is-View');
				$('.SiteMap').removeClass('is-View');
				$('html').removeClass('is-Hidden');
				$('.HeaderSearch').removeClass('is-Open');
				$('.MobileHeaderSearch').removeClass('is-Open');
				$('.PopLanguageSection').removeClass('is-View');
			}
		}
		
		$(document).on('click', '.QuickBarItem-download .QuickBarItem-link, .MobileHeader-link.download', QuickDownload);
		
		function QuickDownloadClose() {
			$('.QuickDownload').removeClass('is-View');
		}
		
		$(document).on('click', '.QuickDownload-close', QuickDownloadClose);
		
		/**
		 * DownloadCustom Select
		 */
		function DownloadCustomSelecter() {
			$(this).parent('.NoBorSelect').toggleClass('is-Active').find('.NoBorSelect-list').stop().slideToggle(140);
			$('.NoBorSelect.is-Active').not( $(this).parent()).removeClass('is-Active').find('.NoBorSelect-list').stop().slideUp(140);
			$('.j-CustomSelect-link.is-Active').not( $(this).parent()).removeClass('is-Active').find('.NoBorSelect-list').stop().slideUp(140);
		}
		
		$(document).on('click', '.NoBorSelect-text', DownloadCustomSelecter);
		
		function  DownloadCustomSelectOption() {
			var _this = $(this);
			var this_text = _this.text();
			_this.closest('.NoBorSelect').removeClass('is-Active');
			_this.parent('.NoBorSelect-item').addClass('is-Active').siblings('.NoBorSelect-item').removeClass('is-Active'); 
			_this.closest('.NoBorSelect-list').stop().slideUp(100).prev('.NoBorSelect-text').text(this_text);
			_this.closest('.NoBorSelect-list').stop().slideUp(100);
			return false;
		}
		
		$(document).on('click', '.NoBorSelect-link', DownloadCustomSelectOption);
		
		$(document).mouseup(function (e){
				if(!$('.NoBorSelect').is(e.target) && $('.NoBorSelect').has(e.target).length === 0){
						$('.NoBorSelect').removeClass('is-Active').children('.NoBorSelect-list').stop().slideUp(140);
				}
		});
		
		/**
		 * BasicDropDown
		 */
		function BasicDropDown() {
			if($(this).closest('.Faq-item').find('.FaqItem').hasClass('is-Current')){
				$(this).closest('.Faq-item').find('.FaqItem').removeClass('is-Current').find('.FaqItemCont').slideUp(120);
			}else{
				$(this).closest('.Faq-item').find('.FaqItem').addClass('is-Current').find('.FaqItemCont').slideDown(120).closest('.Faq-item').siblings('.Faq-item').find('.FaqItem').removeClass('is-Current').find('.FaqItemCont').slideUp(120);
			}
		}
		
		$(document).on('click', '.FaqItemHeader-link', BasicDropDown);
		
		/**
		 * SolutionProduct Slide
		 */
		if($('.SolutionProductSlide').length > 0){
			var SolutionProductSwiper = new Swiper('.SolutionProductSlide-container', {
				observer: true,
				observeParents: true,
				watchOverflow: true,
				slidesPerView: 2,
				spaceBetween: 10,
				navigation: {
					nextEl: '.SolutionProductSlide-next',
					prevEl: '.SolutionProductSlide-prev'
				},
				scrollbar: {
				  el: ".SolutionProductSlide-scrollbar",
				  hide: true,
				},
				breakpoints: {
					1024: {
					slidesPerView: 5,
					spaceBetween: 22,
					},
					767: {
					slidesPerView: 4,
					spaceBetween: 14,
					},
				},
			});
		}
		
		/**
		 * Solution Slide
		 */
		 if($('.eSolutionSlide').length > 0){
			 $(window).on('elementor/frontend/init', function () {
				// wait for elementor pro to load
				elementorFrontend.on('components:init', function () {
					$('.eSolutionSlide').each(function(){
						if($(this).find('.swiper-slide').length > 1){
							const imageCarousel = $(this).find('.swiper-container'),
							swiperInstance = imageCarousel.data('swiper');
							swiperInstance.params.observer = true;
							swiperInstance.params.observeParents = true;
							swiperInstance.params.watchOverflow = true;
							swiperInstance.params.slidesPerView = 3;
							swiperInstance.params.spaceBetween = 8;
							swiperInstance.params.freeMode = true;
							swiperInstance.params.freeModeSticky = false;
							swiperInstance.params.freeModeMomentum = true;
							swiperInstance.params.freeModeMomentumRatio = 0.4;
							swiperInstance.params.freeModeMomentumVelocityRatio = 0.4;
							swiperInstance.params.freeModeMomentumBounce = false;
							
							swiperInstance.params.mousewheel = true;
							swiperInstance.params.mousewheel.eventsTarget = 'swiper-container';

							swiperInstance.params.simulateTouch = false;

							swiperInstance.update(); 
						}
					});
				});
			 });
		 }
		 

		/**
		 * Case Study Slide Sub
		 */
		 if($('.SubCaseStudyTab').length > 0){
			var SubCaseStudyTabPostSection = undefined;
			function SubCaseStudyTabSwiper() {
				var screenWidth = $(window).width();
				if(screenWidth < 768 && SubCaseStudyTabPostSection == undefined) {            
					SubCaseStudyTabPostSection = new Swiper('.SubCaseStudyTab-container', {            
						slidesPerView: "auto",
						freeMode: true,
						scrollbar: {
						  el: ".SubCaseStudyTab-scrollbar",
						  hide: true,
						},
					});
				} else if (screenWidth > 767 && SubCaseStudyTabPostSection != undefined) {
					SubCaseStudyTabPostSection.destroy();
					SubCaseStudyTabPostSection = undefined;        
				}        
			}

			//Swiper plugin initialization
			SubCaseStudyTabSwiper();

			//Swiper plugin initialization on window resize
			$(window).on('resize', function(){
				SubCaseStudyTabSwiper();        
			});
		 }
		 
		 /**
		 * ElementorPopup
		 */
		 if($('.ElementorPopup').length > 0){
			function ElementorPopupClose() {
				$('.ElementorPopup').fadeOut(120);
				var VideoIframe = $(this).closest('.ElementorPopup').find('.elementor-video-iframe');
				var src= VideoIframe.attr('src');
				VideoIframe.attr('src',src);  
			}
			
			$(document).on('click', '.ElementorPopup-closed .elementor-button', ElementorPopupClose);
		  }
		
		/**
		 * ElementorPopupBtn
		 */
		if($('.ElementorPopup').length > 0){
			function ElementorPopupBtn() {
				var Pophref = $(this).attr("href").replace("http://","#");
				$(Pophref).fadeIn(120);
				return false;
			}
			
			$(document).on('click', '.TechnicalPlayBtn .elementor-button-link', ElementorPopupBtn);
		}
		
		/**
		 * juxtapose Slide
		 */
		 if($('#juxtapose-wrapper').length > 0){
			$("#juxtapose-wrapper").on("mousedown click touch touchstart", ".jx-slider", function() {
				$("#juxtapose-wrapper").find(".jx-slider").addClass("hide-instruction");
			});
		}
		
		/**
		 * Single Product Slide
		 */
		 if($('.SingleProductSlide').length > 0){
			var SolutionProductSwiper = new Swiper('.SingleProductSlide-container', {
				observer: true,
				observeParents: true,
				watchOverflow: true,
				slidesPerView: 5,
				spaceBetween: 10,
				navigation: {
					nextEl: '.SingleProductSlide-next',
					prevEl: '.SingleProductSlide-prev'
				},
				scrollbar: {
				  el: ".SingleProductSlide-scrollbar",
				  hide: true,
				},
				breakpoints: {
					1024: {
					slidesPerView: 4,
					spaceBetween: 22,
					},
					767: {
					slidesPerView: 2,
					spaceBetween: 14,
					},
				},
			});
		}
		
		/**
		 * OrageSelect
		 */
		function CustomSelecter() {
			$(this).parent('.OrangeSelect').toggleClass('is-Active').find('.OrangeSelect-list').stop().slideToggle(140);
			$('.OrangeSelect.is-Active').not( $(this).parent()).removeClass('is-Active').find('.OrangeSelect-list').stop().slideUp(140);
			$('.j-OrangeSelect-link.is-Active').not( $(this).parent()).removeClass('is-Active').find('.OrangeSelect-list').stop().slideUp(140);
		}
		
		$(document).on('click', '.OrangeSelect-text', CustomSelecter);
		
		function CustomSelectOption() {
			var _this = $(this);
			var this_text = _this.text();
			_this.closest('.OrangeSelect').removeClass('is-Active');
			_this.parent('.OrangeSelect-item').addClass('is-Current').siblings('.OrangeSelect-item').removeClass('is-Current'); 
			_this.closest('.OrangeSelect-list').stop().slideUp(100).prev('.OrangeSelect-text').text(this_text);
			_this.closest('.OrangeSelect-list').stop().slideUp(100);
			return false;
		}
		
		$(document).on('click', '.OrangeSelect-link', CustomSelectOption);
		
		$(document).mouseup(function (e){
				if(!$('.OrangeSelect').is(e.target) && $('.OrangeSelect').has(e.target).length === 0){
						$('.OrangeSelect').removeClass('is-Active').children('.OrangeSelect-list').stop().slideUp(140);
				}
		});
		
		/**
		 * faqTab
		 */
		 if($('.FaqTab').length > 0){
			var FaqTab = undefined;
			function FaqTabSwiper() {
				var screenWidth = $(window).width();
				if(screenWidth < 1025 && FaqTab == undefined) {            
					FaqTab = new Swiper('.FaqTab-container', {            
						slidesPerView: "auto",
						freeMode: true,
						scrollbar: {
						  el: ".FaqTab-scrollbar",
						  hide: true,
						},
					});
				} else if (screenWidth > 1024 && FaqTab != undefined) {
					FaqTab.destroy();
					FaqTab = undefined;
					jQuery('.FaqTab-wrapper').removeAttr('style');
					jQuery('.FaqTab-slide').removeAttr('style');            
				}        
			}

			//Swiper plugin initialization
			FaqTabSwiper();

			//Swiper plugin initialization on window resize
			$(window).on('resize', function(){
				FaqTabSwiper();        
			});
		 }
		 
		 /**
		 * totalSearchTab
		 */
		 if($('.TotalSearchTab').length > 0){
			var TotalSearchTab = undefined;
			function TotalSearchTabSwiper() {
				var screenWidth = $(window).width();
				if(screenWidth < 1025 && TotalSearchTab == undefined) {            
					TotalSearchTab = new Swiper('.TotalSearchTab-container', {            
						slidesPerView: "auto",
						freeMode: true,
						scrollbar: {
						  el: ".TotalSearchTab-scrollbar",
						  hide: true,
						},
					});
				} else if (screenWidth > 1024 && TotalSearchTab != undefined) {
					TotalSearchTab.destroy();
					TotalSearchTab = undefined;
					jQuery('.TotalSearchTab-wrapper').removeAttr('style');
					jQuery('.TotalSearchTab-slide').removeAttr('style');            
				}        
			}

			//Swiper plugin initialization
			TotalSearchTabSwiper();

			//Swiper plugin initialization on window resize
			$(window).on('resize', function(){
				TotalSearchTabSwiper();        
			});
		 }
		 
		 // TabSelect
		//==================================================*
		function TabSelect() {
			var Select = $(this).parent('.Depth2Tab');
			if(Select.hasClass('is-Open')){
				Select.removeClass('is-Open').children('.Depth2TabNav').stop().slideUp(140);
			}else{
				$('.Depth2Tab').removeClass('is-Open').children('.Depth2TabNav').stop().slideUp(140);
				Select.addClass('is-Open').children('.Depth2TabNav').stop().slideDown(140);
			}
		}
		
		$(document).on('click', '.Depth2Tab-link', TabSelect);
		
		function TabSelectOption() {
			var _this = $(this);
			var this_text = _this.text();
			_this.closest('.Depth2Tab').removeClass('is-Open');
			_this.parent('.Depth2TabNav-item').addClass('is-Current').siblings('.Depth2TabNav-item').removeClass('is-Current'); 
			_this.closest('.Depth2TabNav').stop().slideUp(100).prev('.Depth2Tab-link').text(this_text);
			_this.closest('.Depth2TabNav').stop().slideUp(100);
			return false;
		}
		
		$(document).on('click', '.Depth2TabNav-link', TabSelectOption);
		
		
		$(document).mouseup(function (e){
				if(!$('.Depth2Tab').is(e.target) && $('.Depth2Tab').has(e.target).length === 0){
					$('.Depth2Tab').removeClass('is-Open').children('.Depth2TabNav').stop().slideUp(140);
				}
		});
		
		 // breadcrumbFixed
		//==================================================*
		if($('.breadcrumb').length > 0){
			function breadcrumbFixed() {
					var breadcrumb = $('.breadcrumb').outerHeight();
					var headerHeight = $('.is-HeaderFixed .site-header').outerHeight();
					var topHead = $('.PageTopBanner').outerHeight() - breadcrumb - headerHeight;
					var moTopHead = $('.PageTopBanner').outerHeight() - breadcrumb;
					var thisTop = $(this).scrollTop();
					var moThisTop = $(this).scrollTop();	
					var screenWidth = $(window).width();
					if(screenWidth > 1024){
						if(thisTop > topHead){
							$(".site").addClass("is-Fixed");
						}else{
							$(".site").removeClass("is-Fixed");
						}
					}else{
						if(moThisTop > moTopHead){
							$(".site").addClass("is-Fixed");
						}else{
							$(".site").removeClass("is-Fixed");
						}
					}
			};
			$(window).on('resize', breadcrumbFixed);
			$(window).on('scroll', breadcrumbFixed);
			breadcrumbFixed();
		}
		
