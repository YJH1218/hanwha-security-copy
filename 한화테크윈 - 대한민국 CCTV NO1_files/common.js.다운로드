		// FiexedBreadCrumb
		//==================================================*
		if($('.FixedBreadcrumbbx').length > 0){
			function FixedBreadcrumbbx() {
					var breadcrumb = $('.Depth3Tab').outerHeight();
					var topHead = $('.PageTopBanner').outerHeight() + breadcrumb;
					var moTopHead = $('.PageTopBanner').outerHeight() + breadcrumb;
					var thisTop = $(this).scrollTop();
					var moThisTop = $(this).scrollTop();	
					var screenWidth = $(window).width();
					if(screenWidth > 1024){
						if(thisTop > topHead){
							$(".FixedBreadcrumbbx").addClass("is-View");
						}else{
							$(".FixedBreadcrumbbx").removeClass("is-View");
							$(".FixedBreadcrumbbx .BasicSelect").removeClass("is-Open");
							$('.FixedBreadcrumbbx .BasicSelect').removeClass('is-Open').children('.BasicSelect-list').stop().slideUp(0);
						}
					}else{
						if(moThisTop > moTopHead){
							$(".FixedBreadcrumbbx").addClass("is-View");
						}else{
							$(".FixedBreadcrumbbx").removeClass("is-View");
							$(".FixedBreadcrumbbx .BasicSelect").removeClass("is-Open");
							$('.FixedBreadcrumbbx .BasicSelect').removeClass('is-Open').children('.BasicSelect-list').stop().slideUp(0);
						}
					}
			};
			$(window).on('resize', FixedBreadcrumbbx);
			$(window).on('scroll', FixedBreadcrumbbx);
			FixedBreadcrumbbx();
		}
		
		/**
		 * CompanyTab
		 */
		 if($('.CompanyTab').length > 0){
			var CompanyTab = undefined;
			function CompanyTabSwiper() {
				var screenWidth = $(window).width();
				if(screenWidth < 768 && CompanyTab == undefined) {            
					CompanyTab = new Swiper('.CompanyTab-container', {            
						slidesPerView: "auto",
						freeMode: true,
						scrollbar: {
						  el: ".CompanyTab-scrollbar",
						  hide: true,
						},
					});
				} else if (screenWidth > 767 && CompanyTab != undefined) {
					CompanyTab.destroy();
					CompanyTab = undefined;
					jQuery('.CompanyTab-wrapper').removeAttr('style');
					jQuery('.CompanyTab-slide').removeAttr('style');            
				}        
			}

			//Swiper plugin initialization
			CompanyTabSwiper();

			//Swiper plugin initialization on window resize
			$(window).on('resize', function(){
				CompanyTabSwiper();        
			});
		 }
		 
