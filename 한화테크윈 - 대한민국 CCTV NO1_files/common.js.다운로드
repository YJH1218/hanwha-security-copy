		// Video
		//==================================================*
		function BsVideoPopupOpen() {
			$('.BsVideoPopup').fadeIn(120);
			var targeted_popup_url = $(this).attr('data-video');
			$('#BasicPopupVideoFrame').attr("src", targeted_popup_url).trigger('play');
			$('html').addClass('is-Hidden');
			return false;
		}
		
		$(document).on('click', '.is-Video .ElearningItem-link', BsVideoPopupOpen);
		
		function BsVideoPopupOff() {
			$('.BsVideoPopup').fadeOut(120);
			$('#BasicPopupVideoFrame').attr("src", "").trigger('pause');
			$('html').removeClass('is-Hidden');
			return false;
		}
		
		$(document).on('click', '.BsVideoPopup .BasicPopup-close, .BsVideoPopup .Dim', BsVideoPopupOff);
		
